{
  "name": "payload-template-ts-blog",
  "description": "Payload project created from ts-blog template",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema"
  },
  "dependencies": {
    "@payloadcms/plugin-form-builder": "^1.0.10",
    "@payloadcms/plugin-nested-docs": "^1.0.4",
    "@payloadcms/plugin-search": "^1.0.0",
    "@payloadcms/plugin-seo": "^1.0.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.4.0",
    "payload": "^1.4.1",
    "payload-plugin-password-protection": "^0.0.2",
    "prettier": "^2.8.1",
    "slug": "^8.2.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/express": "^4.17.9",
    "@types/slug": "^5.0.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "prettier-plugin-style-order": "^0.2.2",
    "ts-node": "^9.1.1",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.8.4"
  }
}
